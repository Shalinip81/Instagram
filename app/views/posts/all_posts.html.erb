<p>All Posts</p>
<% @post.each do |f| %>
      <%= image_tag f.post_feed, size: "50" %>
      <br>
      <%= f.Description %>
      <br>
      <span>
      Likes:<%= f.likes.count %>
      Comments: <%= link_to f.comments.count,comments_path(token:@token,user:f.id) %>
    </span>
      <br>
      <br>
  <% end %>

  <%= link_to 'Back', profile_path(token:@token) %>
